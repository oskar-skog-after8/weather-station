#!/bin/sh

cat >index.html <<END
<!DOCTYPE html>
<html leng="en" xmlns="http://www.w3.org/1999/xhtml">
    <head>
        <meta charset="utf-8"/>
        <meta name="viewport" content="width=device-width, initial-scale=1"/>
        <title>Weather station, Pictures</title>
    </head>
    <body>
END

for i in $(seq -w 1 104); do
    echo "<a href='$(echo $i.*)'><img src='thumbnails/$i.jpeg' alt='' width='96' height='96'/></a>" >>index.html
done

cat >>index.html <<END
    </body>
</html>
END

